{
  "name": "todo-app-api",
  "version": "1.0.0",
  "main": "./dist/src/main/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/main/index.js",
    "start:dev": "nodemon -r tsconfig-paths/register ./src/main/index.ts",
    "prebuild": "yarn rimraf ./dist",
    "build": "yarn -r tsconfig-paths/register tsc --build ./tsconfig-build.json",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.44"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "tsconfig-paths": "^3.13.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.2"
  }
